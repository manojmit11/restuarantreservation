Database:
======
rest.admin


CREATE TABLE rest.admin (
  HOTEL_NAME varchar(11) NOT NULL ,
  ADDRESS1 varchar(45) DEFAULT NULL,
  HOURS_OPERATION varchar(10) DEFAULT NULL,
  PRIMARY KEY (HOTEL_NAME)
) 

